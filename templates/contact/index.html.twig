{% extends 'base.html.twig' %}

{% block title %}Contact us{% endblock %}

{% block body %}
<div class="container">
    <div clas="row">
        <div class="col-md-8 offset-md-2">
            {% for message in app.flashes('notice') %}
                <div class="alert alert-info">{{ message }}</div>
